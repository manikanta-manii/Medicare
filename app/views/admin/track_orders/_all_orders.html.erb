<%= stylesheet_link_tag "track_order" %>
<table>
  <thead>
    <tr>
      <th>Order ID</th>
      <th>Customer Name</th>
      <th>Total Price</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
    <% @all_orders.each do |order| %>
      <tr>
        <td><%= order.id %></td>
        <td><%= order.patient.user.name %></td>
        <td><%= order.total_price %>. Rs</td>
        <td><%= button_to "View Order Details",order_path(order.id) ,method: :get,class: "btn btnn btn-info",data: { order: order }%></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= will_paginate @all_orders , renderer: WillPaginate::ActionView::BootstrapLinkRenderer %>