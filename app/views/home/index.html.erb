<%check_user_signed_in do %>
   <% check_if_current_user_is_patient do%>
      <%= render partial:"home/page"%>
   <%end%>
   <%check_if_current_user_is_doctor do%>
      <%= render partial: "doctors/dashboard" %>
   <%end%>
   <%check_if_current_user_is_admin do %>
    <%= render partial: "admin/dashboard" %>
   <%end%>
<%end%>
<%check_user_not_signed_in do %>
  <%= render partial:"home/page"%>
<%end%>

